                      "xfPrepopulateValueType": "custom:program_location_type_data",
                      "xfPrepopulateCustomValue": "pswqf",
                      "xfPrepopulateValueEnabled": true
                    }
                  },
                  {
                    "name": "xf:boolean",
                    "props": {
                      "xfName": "attach_noaa_weather",
                      "xfLabel": "Attach NOAA Weather Forecast Table to Report?",
                      "xfDefaultValueEnabled": true
                    }
                  }
                ]
              }
            },
            {
              "name": "xf:group",
              "props": {
                "xfName": "company",
                "xfLabel": "Company",
                "xfOutputClass": [
