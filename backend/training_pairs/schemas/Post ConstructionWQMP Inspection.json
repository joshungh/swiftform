                "children": [
                  {
                    "name": "xf:string",
                    "props": {
                      "xfName": "company_owner_name",
                      "xfLabel": "Company / Owner Name",
                      "xfInfoType": "custom",
                      "xfPostaction": "update_program_location_type_data_field",
                      "xfInfoEnabled": true,
                      "xfInfoCustomValue": "Information autofilled from the site name ",
                      "xfPostactionEnabled": false,
                      "xfPrepopulateValueType": "location_name",
                      "xfPrepopulateValueEnabled": true
                    }
                  }
                ]
              }
            }
          ]
        }
      },
