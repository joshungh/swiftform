      {
        "name": "xf:page",
        "props": {
          "xfName": "general_information",
          "xfLabel": "General Information",
          "children": [
            {
              "name": "xf:date",
              "props": {
                "xfName": "inspection_date",
                "xfLabel": "Inspection Date",
                "xfPrepopulateValueType": "date_today",
                "xfPrepopulateValueEnabled": true
              }
            },
            {
              "name": "xf:time",
              "props": {
                "xfName": "time",
                "xfLabel": "Time",
                "xfPrepopulateValueType": "time_today",
                "xfPrepopulateValueEnabled": true
              }
            },
            {
              "name": "xf:string",
              "props": {
                "xfName": "inspector",
                "xfLabel": "Inspector",
                "xfPrepopulateValueType": "user_name",
                "xfPrepopulateValueEnabled": true
              }
            },
            {
              "name": "xf:group",
              "props": {
                "xfName": "inspection_type",
                "xfLabel": "Inspection Type",
                "children": [
                  {
                    "name": "xf:select",
                    "props": {
                      "xfName": "pond_condition",
                      "xfLabel": "Pond Condition",
                      "xfOptions": "Dry Pond\nWet Pond",
                      "xfPresetOptionGroup": ""
                    }
                  }
                ]
              }
            },
            {
              "name": "xf:group",
              "props": {
                "xfName": "pond_details",
                "xfLabel": "Pond Details",
                "children": [
                  {
                    "name": "xf:string",
                    "props": {
                      "xfName": "site_pond_name",
                      "xfLabel": "Site / Pond Name",
                      "xfPrepopulateValueType": "location_name",
                      "xfPrepopulateValueEnabled": true
                    }
                  },
                  {
                    "name": "xf:string",
                    "props": {
                      "xfName": "job_no",
                      "xfLabel": "Job # (Site ID)",
