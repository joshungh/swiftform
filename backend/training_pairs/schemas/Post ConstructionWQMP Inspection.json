            {
              "name": "xf:metadata",
              "props": {
                "xfName": "binding__created_at_time",
                "xfValue": "general_information|time"
              }
            },
            {
              "name": "xf:metadata",
              "props": {
                "xfName": "binding__append_report_name",
                "xfValue": "general_information|inspection_type|pond_condition"
              }
            },
            {
              "name": "xf:metadata",
              "props": {
                "xfName": "binding__include_weather_forecast",
                "xfValue": "general_information|pond_details|attach_noaa_weather"
              }
            }
          ]
        }
