                        {
                          "name": "xf:string",
                          "props": {
                            "xfName": "city",
                            "xfLabel": "City",
                            "xfPrepopulateValueType": "location_address_city",
                            "xfPrepopulateValueEnabled": true
                          }
                        },
                        {
                          "name": "xf:string",
                          "props": {
                            "xfName": "state",
                            "xfLabel": "State",
                            "xfPrepopulateValueType": "location_address_state",
                            "xfPrepopulateValueEnabled": true
                          }
                        },
                        {
                          "name": "xf:string",
                          "props": {
                            "xfName": "zip_code",
                            "xfLabel": "Zip Code",
                            "xfPrepopulateValueType": "location_address_zip",
                            "xfPrepopulateValueEnabled": true
                          }
                        }
                      ]
                    }
                  },
                  {
                    "name": "xf:string",
                    "props": {
                      "xfName": "permit_no",
                      "xfLabel": "Permit #",
                      "xfPrepopulateValueType": "custom:program_location_type_data",
                      "xfPrepopulateCustomValue": "regulatory_identifier",
                      "xfPrepopulateValueEnabled": true
                    }
                  },
                  {
                    "name": "xf:date",
                    "props": {
                      "xfName": "renew_date",
                      "xfLabel": "Renew Date",
                      "xfInfoType": "custom",
                      "xfInfoEnabled": true,
                      "xfInfoCustomValue": "Date autofilled from the site's details. Edit Renew Date in the Site Details to update autofill data.",
