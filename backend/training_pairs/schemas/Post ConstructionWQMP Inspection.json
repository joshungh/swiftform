                            "xfDisableDateResolved": true,
                            "xfDeficiencyFieldLabel": "",
                            "xfDisablePhotoDateTime": true,
                            "xfPrepopulateValueType": "current_deficiencies",
                            "xfDisableDateIdentified": false,
                            "xfPrepopulateValueEnabled": true
