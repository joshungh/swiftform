                "xfOutputClass": [
                  "checkboxes-stacked"
                ],
                "xfPrepopulateValueType": "select_last_report",
                "xfPrepopulateValueEnabled": true
              }
            },
            {
              "name": "xf:select",
              "props": {
                "xfName": "qsd_inspection",
                "xfLabel": "QSD on-site visual inspection",
                "xfOptions": "QSD Initial Inspection\nQSD Semi-Annual\nQSD Replacement (QSD)",
